<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <com.google.android.material.chip.Chip
        android:id="@+id/item_station"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/child_margin"
        android:checkable="false"
        app:chipCornerRadius="@dimen/chip_radius"
        app:chipIcon="@drawable/ic_fiber_black"
        app:chipIconEnabled="true"
        app:chipIconSize="@dimen/chip_icon_size"
        app:closeIcon="@drawable/ic_clear_black"
        app:closeIconEnabled="true" />
</FrameLayout>
